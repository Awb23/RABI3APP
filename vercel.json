{
    "version": 2,
    "builds": [
      {
        "src": "djangotest",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/djangotest/wsgi.py",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      }
     
     
    ]
  }
  